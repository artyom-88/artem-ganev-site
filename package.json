{
  "name": "artem-ganev-site",
  "version": "2.0.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://artyom-88.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "analyze": "pnpm run build --mode analyze",
    "build": "vite build",
    "build:ci": "tsc && pnpm run build",
    "build:debug": "vite build --debug",
    "build:dev": "vite build --mode development",
    "depcheck": "pnpm dlx depcheck",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "pre-commit": "tsc && lint-staged",
    "predeploy": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "start": "vite",
    "start:debug": "vite --debug",
    "test": "echo \"vitest run\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@stackoverflow/stacks-icons": "6.0.0",
    "@tanstack/react-query": "5.4.3",
    "antd": "5.11.4",
    "ky": "1.1.3",
    "dayjs": "1.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.18.0",
    "tailwindcss": "3.3.5",
    "zustand": "4.4.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.0.5",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "gh-pages": "6.1.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.12.0",
    "@vitejs/plugin-basic-ssl": "1.0.1",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vitest": "0.3.8",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "15.0.2",
    "postcss": "8.4.31",
    "rollup-plugin-visualizer": "5.9.3",
    "prettier": "3.0.3",
    "sass": "1.69.5",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vitest": "0.34.6"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write --max-warnings 1"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
