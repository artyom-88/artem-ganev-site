{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -o production -d build -t",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "lint": "eslint --fix \"src/**/*.ts?\""
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-redux": "^7.1.18",
    "@types/redux-actions": "^2.6.2",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/parser": "^4.29.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.3",
    "redux-saga-test-plan": "^4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "browserslist": ">=4.16.5",
    "glob-parent": ">=5.1.2",
    "lodash": ">=4.17.21"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/**/*.{js,jsx,ts,tsx}",
      "src/adapter/**/*.{js,jsx,ts,tsx}",
      "src/api/**/*.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/model/**/*.{js,jsx,ts,tsx}",
      "src/reducers/**/*.{js,jsx,ts,tsx}",
      "src/saga/**/*.{js,jsx,ts,tsx}",
      "src/selectors/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
