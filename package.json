{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "audit": "yarn-audit-ci",
    "build": "react-scripts build",
    "deploy": "gh-pages -o production -d build -t",
    "lint": "eslint --fix \"src/**/*.ts?\"",
    "pre-commit": "lint-staged",
    "predeploy": "yarn build",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/styles": "4.11.4",
    "@reduxjs/toolkit": "1.6.2",
    "axios": "0.24.0",
    "moment": "2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@redux-saga/is": "1.1.2",
    "@redux-saga/symbols": "1.1.2",
    "@testing-library/jest-dom": "5.15.0",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-to-json": "1.5.4",
    "@types/jest": "26.0.14",
    "@types/node": "16.11.9",
    "@types/react": "17.0.36",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.1.9",
    "@typescript-eslint/parser": "5.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "redux-saga-test-plan": "4.0.4",
    "source-map-explorer": "2.5.2",
    "typescript": "4.5.2",
    "yarn-audit-ci": "1.2.0"
  },
  "resolutions": {
    "browserslist": ">=4.16.5",
    "glob-parent": ">=5.1.2",
    "immer": ">=9.0.6",
    "lodash": ">=4.17.21",
    "nth-check": ">=2.0.1",
    "path-parse": ">=1.0.7",
    "tar": ">=4.4.18 <5.0.0 || >=5.0.10 <6.0.0 || >=6.1.9",
    "tmpl": ">=1.0.5"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/**/*.{js,jsx,ts,tsx}",
      "src/adapter/**/*.{js,jsx,ts,tsx}",
      "src/api/**/*.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/model/**/*.{js,jsx,ts,tsx}",
      "src/reducers/**/*.{js,jsx,ts,tsx}",
      "src/saga/**/*.{js,jsx,ts,tsx}",
      "src/selectors/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
