{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "audit": "yarn-audit-ci",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "deploy": "gh-pages -o production -d build -t",
    "lint": "eslint --fix \"src/**/*.ts?\"",
    "pre-commit": "lint-staged",
    "predeploy": "yarn build",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.3.1",
    "@mui/material": "5.4.0",
    "@mui/styles": "5.3.0",
    "@reduxjs/toolkit": "1.7.2",
    "axios": "0.25.0",
    "moment": "2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.1",
    "redux": "4.1.2",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/plugin-syntax-flow": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.16.7",
    "@redux-saga/is": "1.1.2",
    "@redux-saga/symbols": "1.1.2",
    "@testing-library/jest-dom": "5.16.2",
    "@types/enzyme": "3.10.11",
    "@types/enzyme-to-json": "1.5.4",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.13",
    "@types/react": "17.0.39",
    "@types/react-helmet": "6.1.5",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.22",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/parser": "5.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "autoprefixer": "10.4.2",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.2",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "react-scripts": "5.0.0",
    "redux-saga-test-plan": "4.0.4",
    "source-map-explorer": "2.5.2",
    "typescript": "4.5.5",
    "yarn-audit-ci": "1.2.0"
  },
  "resolutions": {
    "@babel/runtime": ">=7.16.7",
    "browserslist": ">=4.16.5",
    "follow-redirects": ">=1.14.7",
    "glob-parent": ">=5.1.2",
    "immer": ">=9.0.6",
    "lodash": ">=4.17.21",
    "nth-check": ">=2.0.1",
    "path-parse": ">=1.0.7",
    "postcss": " >=8.2.13",
    "tar": ">=4.4.18 <5.0.0 || >=5.0.10 <6.0.0 || >=6.1.9",
    "tmpl": ">=1.0.5"
  },
  "lint-staged": {
    "src/**/*.{css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/common/**/*.{ts,tsx}",
      "src/features/**/*.{ts,tsx}"
    ]
  }
}
