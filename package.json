{
  "name": "artem-ganev-site",
  "version": "2.0.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://artyom-88.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "analyze": "pnpm run build --mode analyze",
    "build": "vite build",
    "build:ci": "tsc && pnpm build",
    "build:debug": "vite build --debug",
    "build:dev": "vite build --mode development",
    "depcheck": "pnpm dlx depcheck",
    "deploy:local": "pnpm build:ci && echo $(git rev-parse HEAD)  > ./dist/version && pnpm gh-pages -d dist -u \"Art—ëm <artem_ganev@epam.com>\" -m \"Manual deploy\"",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "pre-commit": "tsc && lint-staged",
    "predeploy": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "preview": "vite preview",
    "start": "vite",
    "start:debug": "vite --debug",
    "test": "echo \"vitest run\"",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@ant-design/icons": "5.3.6",
    "@stackoverflow/stacks-icons": "2.27.0",
    "@tanstack/react-query": "5.50.1",
    "antd": "5.16.4",
    "dayjs": "1.11.10",
    "ky": "1.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.4",
    "react-router-dom": "6.23.0",
    "tailwindcss": "3.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "@tanstack/eslint-plugin-query": "5.28.11",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.4",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.5.2",
    "autoprefixer": "10.4.19",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-vitest": "0.3.26",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.75.0",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-svgo": "1.4.0",
    "vitest": "1.5.2"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write --max-warnings 1"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
