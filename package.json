{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -o production -d build -t",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "lint": "eslint --fix \"src/**/*.ts?\""
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "axios": "0.19.2",
    "moment": "2.26.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.3.0",
    "@testing-library/user-event": "12.0.7",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.3",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.13",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-redux": "7.1.9",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5",
    "@typescript-eslint/parser": "3.3.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.16.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "gh-pages": "3.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "react-scripts": "3.4.1",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "sass": "1.26.8",
    "typescript": "3.9.5"
  },
  "resolutions": {
    "**/**/minimist": "^1.2.2",
    "**/**/yargs-parser": "^18.1.2"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/**/*.{js,jsx,ts,tsx}",
      "src/adapter/**/*.{js,jsx,ts,tsx}",
      "src/api/**/*.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/container/**/*.{js,jsx,ts,tsx}",
      "src/model/**/*.{js,jsx,ts,tsx}",
      "src/reducers/**/*.{js,jsx,ts,tsx}",
      "src/saga/**/*.{js,jsx,ts,tsx}",
      "src/selectors/**/*.{js,jsx,ts,tsx}",
      "src/api/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
