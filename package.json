{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -o production -d build -t",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "lint": "eslint --fix \"src/**/*.ts?\""
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "0.20.0",
    "moment": "2.29.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.7",
    "@types/enzyme": "3.10.7",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.4",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.8",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@types/react-redux": "7.1.9",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5",
    "@typescript-eslint/parser": "4.15.1",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "react-scripts": "3.4.3",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "typescript": "4.0.3"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/actions/**/*.{js,jsx,ts,tsx}",
      "src/adapter/**/*.{js,jsx,ts,tsx}",
      "src/api/**/*.{js,jsx,ts,tsx}",
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/model/**/*.{js,jsx,ts,tsx}",
      "src/reducers/**/*.{js,jsx,ts,tsx}",
      "src/saga/**/*.{js,jsx,ts,tsx}",
      "src/selectors/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
