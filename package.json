{
  "name": "artem-ganev-site",
  "version": "1.1.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://www.artemganev.site",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.22.x"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "audit:ci": "yarn-audit-ci",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint --fix \"src/**/*.ts?\"",
    "pre-commit": "lint-staged",
    "predeploy": "yarn build",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.5.1",
    "@mui/material": "5.6.0",
    "@mui/styles": "5.5.3",
    "@reduxjs/toolkit": "1.8.1",
    "@stackoverflow/stacks-icons": "^2.27.0",
    "axios": "1.6.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "4.1.2",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-syntax-flow": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@redux-saga/is": "1.1.2",
    "@redux-saga/symbols": "1.1.2",
    "@testing-library/jest-dom": "5.16.4",
    "@types/enzyme": "3.10.11",
    "@types/enzyme-to-json": "1.5.4",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "5.17.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "autoprefixer": "10.4.4",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "react-scripts": "5.0.1",
    "redux-saga-test-plan": "4.0.4",
    "source-map-explorer": "2.5.2",
    "typescript": "4.6.3",
    "yarn-audit-ci": "1.2.0"
  },
  "resolutions": {
    "minimatch": ">=3.0.5",
    "nth-check": ">=2.0.1"
  },
  "lint-staged": {
    "src/**/*.{css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{ts,tsx}",
      "src/common/**/*.{ts,tsx}",
      "src/features/**/*.{ts,tsx}"
    ]
  }
}
