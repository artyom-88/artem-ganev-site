{
  "name": "artem-ganev-site",
  "version": "1.1.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://www.artemganev.site",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.22.x"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "audit": "yarn-audit-ci",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint --fix \"src/**/*.ts?\"",
    "pre-commit": "lint-staged",
    "predeploy": "yarn build",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/styles": "^5.10.2",
    "@reduxjs/toolkit": "^1.8.5",
    "@stackoverflow/stacks-icons": "^2.27.0",
    "axios": "^0.27.2",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@redux-saga/is": "^1.1.3",
    "@redux-saga/symbols": "^1.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^5.35.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "autoprefixer": "^10.4.8",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "redux-saga-test-plan": "^4.0.5",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.8.2",
    "yarn-audit-ci": "^1.2.0"
  },
  "lint-staged": {
    "src/**/*.{css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{ts,tsx}",
      "src/common/**/*.{ts,tsx}",
      "src/features/**/*.{ts,tsx}"
    ]
  }
}
