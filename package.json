{
  "name": "artem-ganev-site",
  "version": "2.0.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://artyom-88.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "analyze": "pnpm run build --mode analyze",
    "build": "vite build",
    "build:ci": "tsc && pnpm build",
    "build:debug": "vite build --debug",
    "build:dev": "vite build --mode development",
    "depcheck": "pnpm dlx depcheck",
    "deploy:local": "pnpm build:ci && echo $(git rev-parse HEAD)  > ./dist/version && pnpm gh-pages -d dist -u \"Artem Ganev <a.a.ganev@gmail.com>\" -m \"Manual deploy\"",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "pre-commit": "tsc && lint-staged",
    "predeploy": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "start": "vite",
    "start:debug": "vite --debug",
    "test": "echo \"vitest run\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@stackoverflow/stacks-icons": "2.27.0",
    "@tanstack/react-query": "5.8.7",
    "antd": "5.12.5",
    "dayjs": "1.11.10",
    "ky": "1.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.20.0",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.12.1",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "18.18.13",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.16.0",
    "@vitejs/plugin-basic-ssl": "1.0.2",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.0.4",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vitest": "0.3.18",
    "gh-pages": "6.1.0",
    "husky": "8.0.3",
    "jsdom": "23.0.0",
    "lint-staged": "15.2.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "rollup-plugin-visualizer": "5.11.0",
    "sass": "1.69.5",
    "typescript": "5.2.2",
    "vite": "5.0.7",
    "vite-plugin-svgo": "1.4.0",
    "vitest": "1.0.4"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write --max-warnings 1"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
